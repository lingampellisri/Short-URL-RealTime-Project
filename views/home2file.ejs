
  <table border="1">
    <thead>
      <tr>
        <th>SNo</th>
        <th>Short ID</th>
        <th>URL</th>
        <th>Visit Count</th>
      </tr>
    </thead>
    <tbody>
      <% urls.forEach((url, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= url.shortId %></td>
          <td><%= url.redirectURL %></td>
          <td><%= url.visitHistory.length %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>